#!/bin/sh

set -e


#  ____  _             _     ____             _             
# / ___|| |_ __ _ _ __| |_  |  _ \  ___   ___| | _____ _ __ 
# \___ \| __/ _` | '__| __| | | | |/ _ \ / __| |/ / _ \ '__|
#  ___) | || (_| | |  | |_  | |_| | (_) | (__|   <  __/ |   
# |____/ \__\__,_|_|   \__| |____/ \___/ \___|_|\_\___|_|   


OS="$(uname -s)"

case "$OS" in
	"Darwin")
		open -a docker
	;;
	*)
		echo "$OS not supported"
		exit 1
	;;
esac
